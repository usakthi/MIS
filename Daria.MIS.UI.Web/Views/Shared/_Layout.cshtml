<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="MIS">
    <title></title>
    <!-- =============== VENDOR STYLES ===============-->
    <link href="@Styles.Url("~/content/angle/Vendor/fontawesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <!-- =============== PAGE VENDOR STYLES =============== -->
    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}
    <!-- =============== APP STYLES ===============-->
    <link href="@Styles.Url("~/Content/angle/css/app.css")" rel="stylesheet" type="text/css" id="maincss" />

</head>
<body class="layout-fixed">
    <div class="wrapper">
        <!-- top navbar-->
        <header class="topnavbar-wrapper">
            @Html.Partial("_TopNavbar")@*, (List<Daria.MIS.Core.Entities.MenuItem>)ViewBag.UserMenu)*@
        </header>
        <!-- sidebar-->
        <aside class="aside">
            @Html.Partial("_SideMenubar", (List<Daria.MIS.Core.Entities.MenuItem>)ViewBag.UserMenu)
        </aside>
        <section>
            <!-- Page content-->
            @RenderBody()

        </section>
        <!-- Page footer-->
        <footer>
            @Html.Partial("_Footer")
        </footer>
    </div>
    @if (IsSectionDefined("BodyArea"))
    {@RenderSection("BodyArea", required: false)}
    <!-- =============== VENDOR SCRIPTS ===============-->
    @Scripts.Render("~/bundles/modernizr")
    
    <script src="@Scripts.Url("~/scripts/jquery-2.1.3.min.js")" type="text/javascript"></script>
    <script src="@Scripts.Url("~/scripts/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Scripts.Url("~/scripts/angular.min.js")" type="text/javascript"></script>
    <script src="@Scripts.Url("~/scripts/angular-resource.min.js")" type="text/javascript"></script>
    <script src="@Scripts.Url("~/scripts/angular-route.min.js")" type="text/javascript"></script>
    

    @Scripts.Render("~/bundles/Common")

    <!-- =============== PAGE VENDOR SCRIPTS ===============-->
    @RenderSection("scripts", required: false)
    <!-- =============== APP SCRIPTS ===============-->

</body>
</html>